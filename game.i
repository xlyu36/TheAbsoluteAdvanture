    player.height = 24;
    player.aniCounter = 0;
    player.aniState = SPRITERIGHT;
    player.curFrame = 0;
    player.numFrames = 3;
    player.isJumping = 0;
    player.hasJumped = 0;
}
void updatePlayer() {

    if (player.aniState != SPRITEIDLE) {
        player.prevAniState = player.aniState;
        player.aniState = SPRITEIDLE;
    }
    if (player.aniCounter % 5 == 0) {
        player.curFrame++;
        if (player.curFrame == player.numFrames) {
            player.curFrame = 0;
        }
    }


    if((~((*(volatile unsigned short *)0x04000130)) & ((1<<5)))) {
        player.aniState = SPRITELEFT;
        if (player.worldCol > 0
                && COLLISIONMAP[((player.worldRow)*(1024)+(player.worldCol - 1))] != 0
                && COLLISIONMAP[((player.worldRow + player.height / 2)*(1024)+(player.worldCol - 1))] != 0
                && COLLISIONMAP[((player.worldRow + player.height - 1)*(1024)+(player.worldCol - 1))] != 0) {
            player.worldCol -= player.cdel;
        }
        if (screenBlock >= 28 && hOff > 0 && (player.worldCol - playerhOff) < (240 / 2)) {
            hOff -= player.cdel;
            playerhOff -= player.cdel;
        }
    }


    if((~((*(volatile unsigned short *)0x04000130)) & ((1<<4)))) {
        player.aniState = SPRITERIGHT;

        if ((player.worldCol + player.width) < 1024
                && COLLISIONMAP[((player.worldRow)*(1024)+(player.worldCol + player.width))] != 0
                && COLLISIONMAP[((player.worldRow + player.height / 2)*(1024)+(player.worldCol + player.width))] != 0
                && COLLISIONMAP[((player.worldRow + player.height - 1)*(1024)+(player.worldCol + player.width))] != 0) {


            player.worldCol += player.cdel;
        }
        if (screenBlock < 31 && (hOff < (1024 - 240)) && (player.worldCol - playerhOff) > (240 / 2)) {
            hOff += player.cdel;
            playerhOff += player.cdel;
        }
    }


    if ((!(~(oldButtons)&((1<<6))) && (~buttons & ((1<<6))))
            && COLLISIONMAP[((player.worldRow + player.height)*(1024)+(player.worldCol + player.width / 2))] != 1) {
        player.isJumping = 1;
    }
    if (player.isJumping) {
        if ((jumpCount < 35 && player.worldRow > 0)
                && COLLISIONMAP[((player.worldRow - 1)*(1024)+(player.worldCol))] != 0
                && COLLISIONMAP[((player.worldRow - 1)*(1024)+(player.worldCol + player.width - 1))] != 0
                && COLLISIONMAP[((player.worldRow - 1)*(1024)+(player.worldCol + player.width / 2))] != 0) {
            player.worldRow -= player.rdel;
            jumpCount++;
        } else {
            player.isJumping = 0;
            jumpCount = 0;
        }
    } else {
        if (COLLISIONMAP[((player.worldRow + player.height + player.rdel - 1)*(1024)+(player.worldCol))] != 0
                && COLLISIONMAP[((player.worldRow + player.height - 1 + player.rdel)*(1024)+(player.worldCol + player.width - 1))] != 0
                && COLLISIONMAP[((player.worldRow + player.height - 1 + player.rdel)*(1024)+(player.worldCol+ player.width / 2))] != 0) {
            player.worldRow += player.rdel;
        } else {
            player.hasJumped = 0;
        }
        if (player.worldRow > 135) {
            life = 0;
            iq -= 50;
        }
    }


    if (player.aniState == SPRITEIDLE) {
        player.curFrame = 0;
        player.aniState = player.prevAniState;
    } else {
        player.aniCounter++;
    }



    if (hOff > 256) {
        screenBlock++;
        hOff -= 256;
        (*(volatile unsigned short*)0x4000008) = ((0)<<2) | ((screenBlock)<<8) | (1<<14) | (0<<7);
    } else if (hOff < 0) {
        screenBlock--;
        hOff += 256;
        (*(volatile unsigned short*)0x4000008) = ((0)<<2) | ((screenBlock)<<8) | (1<<14) | (0<<7);
    }


    if (playerhOff > 512) {
        playerhOff -= 512;
    } else if (playerhOff < 0) {
        playerhOff += 512;
    }

    (*(volatile unsigned short *)0x04000010) = hOff;


    if (COLLISIONMAP[((player.worldRow)*(1024)+(player.worldCol - 1))] == 2
            || COLLISIONMAP[((player.worldRow + player.height / 2)*(1024)+(player.worldCol - 1))] == 2
            || COLLISIONMAP[((player.worldRow + player.height - 1)*(1024)+(player.worldCol - 1))] == 2
            || COLLISIONMAP[((player.worldRow)*(1024)+(player.worldCol + player.width))] == 2
            || COLLISIONMAP[((player.worldRow + player.height / 2)*(1024)+(player.worldCol + player.width))] == 2
            || COLLISIONMAP[((player.worldRow + player.height - 1)*(1024)+(player.worldCol + player.width))] == 2
            || COLLISIONMAP[((player.worldRow + player.height + player.rdel - 1)*(1024)+(player.worldCol))] == 2
            || COLLISIONMAP[((player.worldRow + player.height - 1 + player.rdel)*(1024)+(player.worldCol + player.width - 1))] == 2
            || COLLISIONMAP[((player.worldRow + player.height - 1 + player.rdel)*(1024)+(player.worldCol+ player.width / 2))] == 2) {
        life = 0;
        iq -= 50;
    }
}
